function bmi(weight, height) {
  let num = weight / height **2
  if (num <= 18.5) {return 'Underweight'}
  else if (num <= 25) {return 'Normal'}
  else if (num <= 30) {return 'Overweight'}
  else {return 'Obese'}
}
