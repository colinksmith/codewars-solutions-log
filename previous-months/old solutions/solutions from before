function howMuchILoveYou(nbPetals) {
    let remainder = nbPetals % 6 
    if (remainder === 1) {
      return 'I love you'
    } else if(remainder === 2) {
      return 'a little'
    } else if(remainder === 3) {
      return 'a lot'
    } else if(remainder === 4) {
      return 'passionately'
    } else if(remainder === 5) {
      return 'madly'
    } else {
      return 'not at all'
    }
}
